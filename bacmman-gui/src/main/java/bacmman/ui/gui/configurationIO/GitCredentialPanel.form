<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="bacmman.ui.gui.configurationIO.GitCredentialPanel">
  <grid id="27dc6" binding="credentialPanel" layout-manager="GridLayoutManager" row-count="2" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="0"/>
    <constraints>
      <xy x="20" y="20" width="500" height="129"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="b267e" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
        <border type="none" title="Username"/>
        <children>
          <component id="d83ff" class="javax.swing.JTextField" binding="username">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties>
              <text value="bacmman"/>
              <toolTipText value="Enter the username of a github account containing files to browse. Right Click: display recent list"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="5b658" class="javax.swing.JButton" binding="generateToken">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Generate Token"/>
          <toolTipText value="token will be stored encrypted using the password"/>
        </properties>
      </component>
      <grid id="e5459" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
        <border type="none" title="Password"/>
        <children>
          <component id="a7642" class="javax.swing.JPasswordField" binding="password">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties>
              <toolTipText value="&lt;html&gt;Enter a password in order to store a github token or to load a previously stored token. &lt;br /&gt;If no password is set, only publicly available files will be shown and saving or updating local files to the remote server won't be possible. &lt;br /&gt;This password will be recorded in memory untill bacmann is closed, and will not be saved on the disk.&lt;/html&gt;"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="a8cdf" class="javax.swing.JButton" binding="loadToken">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Connect"/>
          <toolTipText value="load a previously stored token and connect to github account"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
